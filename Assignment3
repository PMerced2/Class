	printf("Insert value 1: \n");
	scanf("%.2f",val1);
		printf("Insert value 2: \n");
		scanf("%.2f",val2);
			printf("Value 1 devided Value 2 equals:%.2f\n", val1 / val2);
			val3=val1 / val2;
			val4=val1 + val2 + val3;
			val5 = val1 *val1;
